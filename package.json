{
  "name": "bsc-arbitrage-bot",
  "version": "1.0.0",
  "description": "Flash loan arbitrage bot for PancakeSwap V2/V3 on BSC",
  "main": "bscLiquidationBot.js",
  "scripts": {
    "start": "node bscLiquidationBot.js",
    "dashboard": "node botWithDashboard.js",
    "pm2:start": "pm2 start bscBotMainnet.js --name bsc-arb-bot",
    "pm2:stop": "pm2 stop bsc-arb-bot",
    "pm2:restart": "pm2 restart bsc-arb-bot",
    "pm2:logs": "pm2 logs bsc-arb-bot",
    "pm2:status": "pm2 status",
    "deploy": "npx hardhat run scripts/deploy-bsc.js --network bsc",
    "deploy:testnet": "npx hardhat run scripts/deploy-bsc.js --network bscTestnet",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test"
  },
  "keywords": [
    "bsc",
    "arbitrage",
    "flash-loan",
    "pancakeswap",
    "defi",
    "trading-bot"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "p-limit": "^7.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "hardhat": "^2.26.0",
    "hardhat-gas-reporter": "^1.0.10"
  }
}
